<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>conan-runner</name>
    <display-name>Conan.io package manager support</display-name>
    <version>@Version@</version>
    <description>Build and upload Conan artifacts for each set of options</description>
    <download-url>Plugin download URL</download-url>
    <email>david.zemon@redlion.net</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
    </vendor>
  </info>
  <deployment use-separate-classloader="true"/>
</teamcity-plugin>
